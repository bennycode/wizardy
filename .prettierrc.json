{
  "arrowParens": "avoid",
  "bracketSpacing": false,
  "jsxBracketSameLine": true,
  "overrides": [
    {
      "files": ["*.json"],
      "options": {
        "printWidth": 200
      }
    }
  ],
  "printWidth": 120,
  "proseWrap": "never",
  "requirePragma": false,
  "semi": true,
  "singleQuote": true,
  "tabWidth": 2,
  "trailingComma": "es5",
  "useTabs": false
}
